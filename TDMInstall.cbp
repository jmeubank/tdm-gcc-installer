<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="TDMInstall" />
		<Option makefile_is_custom="1" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="all">
				<Option type="4" />
				<Option compiler="gcc" />
				<Option host_application="tdm-mingw-setup.exe" />
				<Option run_host_application_in_terminal="0" />
				<Compiler>
					<Add option="-Os" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<VirtualTargets>
			<Add alias="All" targets="all;" />
		</VirtualTargets>
		<Compiler>
			<Add option="-Wall" />
			<Add directory="$(#nsissrc)" />
			<Add directory="." />
			<Add directory="zlib" />
			<Add directory="bzip2" />
		</Compiler>
		<Linker>
			<Add library="gdi32" />
			<Add library="comctl32" />
			<Add directory="$(#tinyxml)" />
		</Linker>
		<Unit filename="GCC-VERSION.txt" />
		<Unit filename="Makefile" />
		<Unit filename="README.TDMInstall.txt" />
		<Unit filename="SETUP-VERSION.txt" />
		<Unit filename="archive.cpp" />
		<Unit filename="arcout.template.txt" />
		<Unit filename="arcreg.template.txt" />
		<Unit filename="boost/LICENSE_1_0.txt" />
		<Unit filename="boost/MODIFIED_BOOST.txt" />
		<Unit filename="componentstree.cpp" />
		<Unit filename="componentstree.hpp" />
		<Unit filename="config.h" />
		<Unit filename="ctemplate.cpp">
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Unit filename="inner-manifest-tdm32.txt" />
		<Unit filename="inner-manifest-tdm64.txt" />
		<Unit filename="install_manifest.cpp" />
		<Unit filename="install_manifest.hpp" />
		<Unit filename="main.nsh">
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Unit filename="nsis_interface.cpp" />
		<Unit filename="nsis_interface.hpp" />
		<Unit filename="plugins/README-plugins.txt">
			<Option target="&lt;{~None~}&gt;" />
		</Unit>
		<Unit filename="ref.hpp" />
		<Unit filename="setup_full_tdm32.nsi" />
		<Unit filename="setup_full_tdm64.nsi" />
		<Unit filename="setup_webdl.nsi" />
		<Unit filename="tdminst_res.h" />
		<Unit filename="tdminst_res.rc">
			<Option compilerVar="WINDRES" />
		</Unit>
		<Unit filename="tdminstall.cpp" />
		<Unit filename="tinystr.cpp" />
		<Unit filename="tinystr.h" />
		<Unit filename="tinyxml.cpp" />
		<Unit filename="tinyxml.h" />
		<Unit filename="tinyxmlerror.cpp" />
		<Unit filename="tinyxmlparser.cpp" />
		<Extensions>
			<code_completion />
			<debugger />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
